{
  "name": "SudokuTrainer",
  "short_name": "SudokuTrainer",
  "description": "Diese App ist ein Trainer f√ºr klassisches Sudoku. Sie besteht aus drei Komponenten, (1) dem Sudoku-Solver, (2) dem Sudoku-Generator und (3) der Sudoku-Datenbank.",

  "id": "SudokuTrainerPWAManifest",
  "scope": "/sudokuSolver/",
  "start_url": "/sudokuSolver/index.html",

  "background_color": "#ffffff",
  "theme_color": "#ffffff",

  "display": "standalone",

  "icons": [
    {
      "src": "./images/s256.png",
      "type": "image/png",
      "sizes": "256x256"
    },
    {
      "src": "./images/s512.png",
      "type": "image/png",
      "sizes": "512x512"
    },
    {
      "src": "./images/s1024.png",
      "type": "image/png",
      "sizes": "1024x1024"
    }
  ],

  "screenshots": [
    {
      "src": "./images/screenshot-282x563.jpg",
      "sizes": "282x568",
      "type": "image/jpeg",
      "form_factor": "narrow",
      "label": "sudoku-trainer"
    },
    {
      "src": "./images/screenshot-527x434.jpg",
      "sizes": "527x434",
      "type": "image/jpeg",
      "form_factor": "wide",
      "label": "sudoku-trainer"
    }
  ],

  "share_target": {
    "action": "/sudokuSolver/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "files": [
        {
          "name": "file",
          "accept": ["text/plain", ".txt", ".sudoku"]
        }
      ]
    }
  },

  

  "file_handlers": [
    {
      "action": "/sudokuSolver/",
      "accept": {
        "text/*": [".txt", ".sudoku"]
      }
    }
  ]
}
